{
  "name": "gemfilelock",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "path_section"
          },
          {
            "type": "SYMBOL",
            "name": "git_section"
          },
          {
            "type": "SYMBOL",
            "name": "gem_section"
          },
          {
            "type": "SYMBOL",
            "name": "platforms"
          },
          {
            "type": "SYMBOL",
            "name": "dependencies"
          },
          {
            "type": "SYMBOL",
            "name": "ruby_version"
          },
          {
            "type": "SYMBOL",
            "name": "bundled_with"
          }
        ]
      }
    },
    "path_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "PATH"
        },
        {
          "type": "SYMBOL",
          "name": "section_content"
        }
      ]
    },
    "git_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "GIT"
        },
        {
          "type": "SYMBOL",
          "name": "section_content"
        }
      ]
    },
    "gem_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "GEM"
        },
        {
          "type": "SYMBOL",
          "name": "section_content"
        }
      ]
    },
    "section_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "key_value"
          }
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "key_value"
              }
            },
            {
              "type": "SYMBOL",
              "name": "specs"
            }
          ]
        }
      ]
    },
    "key_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "specs": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "specs:"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "gem_entry_with_dependencies"
          }
        }
      ]
    },
    "platforms": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "PLATFORMS"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "platform_name"
          }
        }
      ]
    },
    "dependencies": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "DEPENDENCIES"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "dependency"
          }
        }
      ]
    },
    "ruby_version": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "RUBY VERSION"
        },
        {
          "type": "SYMBOL",
          "name": "ruby_version_entry"
        }
      ]
    },
    "bundled_with": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "BUNDLED WITH"
        },
        {
          "type": "SYMBOL",
          "name": "bundled_with_entry"
        }
      ]
    },
    "ruby_version_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "ruby"
        },
        {
          "type": "SYMBOL",
          "name": "version"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "ruby_patch"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "ruby_patch": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "p"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "bundled_with_entry": {
      "type": "SYMBOL",
      "name": "version"
    },
    "gem_entry_with_dependencies": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "gem_entry"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_indent"
                },
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "SYMBOL",
                    "name": "dependency"
                  }
                },
                {
                  "type": "SYMBOL",
                  "name": "_dedent"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "gem_entry": {
      "type": "PREC_RIGHT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "gem_name"
            }
          },
          {
            "type": "FIELD",
            "name": "version",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "("
                },
                {
                  "type": "FIELD",
                  "name": "value",
                  "content": {
                    "type": "SYMBOL",
                    "name": "version"
                  }
                },
                {
                  "type": "STRING",
                  "value": ")"
                }
              ]
            }
          }
        ]
      }
    },
    "dependency": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "gem_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "constraint",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "("
                  },
                  {
                    "type": "FIELD",
                    "name": "value",
                    "content": {
                      "type": "SYMBOL",
                      "name": "constraint"
                    }
                  },
                  {
                    "type": "STRING",
                    "value": ")"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "constraint": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "tilde"
              },
              {
                "type": "SYMBOL",
                "name": "comparison"
              },
              {
                "type": "SYMBOL",
                "name": "exact"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "tilde": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "~>"
        },
        {
          "type": "SYMBOL",
          "name": "version"
        }
      ]
    },
    "comparison": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ">="
            },
            {
              "type": "STRING",
              "value": "<="
            },
            {
              "type": "STRING",
              "value": ">"
            },
            {
              "type": "STRING",
              "value": "<"
            },
            {
              "type": "STRING",
              "value": "="
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "version"
        }
      ]
    },
    "exact": {
      "type": "SYMBOL",
      "name": "version"
    },
    "version": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "number"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "."
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
    },
    "gem_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_!+-]+"
    },
    "string": {
      "type": "PATTERN",
      "value": ".+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "platform_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_-]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_indent"
    },
    {
      "type": "SYMBOL",
      "name": "_dedent"
    }
  ],
  "inline": [],
  "supertypes": []
}

